{
  "name": "pressmodo-onboarding",
  "version": "1.0.2",
  "main": "index.js",
  "repository": "https://github.com/Pressmodo/onboarding",
  "author": "Alessandro Tesoro",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "npm run prod && npm run i18n && npm run export",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "export": "yarn prod && composer update --prefer-dist --no-dev && cross-env export=1 NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "i18n": "npm run i18n:textdomain && npm run i18n:pot",
    "i18n:textdomain": "npx wpi18n addtextdomain --exclude=vendor,node_modules",
    "i18n:pot": "npx wpi18n makepot"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.5",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "fs": "^0.0.1-security",
    "laravel-mix": "^5.0.9",
    "node-wp-i18n": "^1.2.3",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^30.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@wordpress/i18n": "^3.16.0",
    "axios": "^0.21.0",
    "lodash.has": "^4.5.2",
    "moment": "^2.29.1",
    "qs": "^6.9.4",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
